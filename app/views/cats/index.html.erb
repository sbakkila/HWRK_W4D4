<section class="cat-index-holder">
  <nav>
    <h1>ALL OF THE CATS IN THE WORLD</h1>
    <a href="<%= new_cat_url %>">
      Add Cat
      <i class="fa fa-plus" aria-hidden="true"></i>
    </a>
  </nav>
  <section class="cat-index">
    <% @cats.each do |cat| %>
      <div class="cat-index-item">
        <div>
          <img src="http://placekitten.com/g/231/231" >
          <br /><%= link_to "Rent #{cat.name}", cat_url(cat) %>
        </div>
      </div>
    <% end %>
  </section>
</section>

<% content_for :footer do %>
  I bet you didn't know we had a list of all of the cats in the world.
<% end %>
